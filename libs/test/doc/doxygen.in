# template configuration for Doxygen/Boost.test
# ${BOOST_ROOT_FOLDER} will be replaced by the root of Boost

ALWAYS_DETAILED_SEC = YES
AUTOLINK_SUPPORT = YES
BRIEF_MEMBER_DESC = YES
CASE_SENSE_NAMES = YES
ENABLE_PREPROCESSING = YES
EXAMPLE_PATH = ${BOOST_ROOT_FOLDER}/libs/test/doc/examples
EXPAND_ONLY_PREDEF = YES
EXTRACT_ALL = YES
EXTRACT_PRIVATE = NO
GENERATE_HTML = NO
GENERATE_XML = YES
HIDE_UNDOC_CLASSES = NO
HIDE_UNDOC_MEMBERS = NO
INCLUDE_PATH = ${BOOST_ROOT_FOLDER}/libs/test/include
INLINE_INHERITED_MEMB = YES
INTERNAL_DOCS = NO
MACRO_EXPANSION = YES
MULTILINE_CPP_IS_BRIEF = YES
PREDEFINED = "BOOST_TEST_DECL=" \
             "BOOST_TEST_DOXYGEN_DOC__=1"

REPEAT_BRIEF = YES
SEARCH_INCLUDES = YES
SHORT_NAMES = YES
SUBGROUPING = YES
XML_OUTPUT = ${DOXYGEN_OUTPUT_XML_FOLDER}
OUTPUT_DIRECTORY = "${DOXYGEN_OUTPUT_FOLDER}"
GENERATE_LATEX = NO
